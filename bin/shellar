#!/bin/sh

main() {
    local subcommand="${1}"
    shift 1
    local args="${@}"
    if env PATH=${shellar}/libexec:${PATH} type "shellar-${subcommand}" > /dev/null
    then
        env PATH=${shellar}/libexec:${PATH}  "shellar-${subcommand}" "${args}"
    else
        echo "command not found"
    fi

}

main "$@"
